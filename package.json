{
	"name": "pakaiwa",
	"type": "module",
	"module": "src/app.ts",
	"main": "dist/app.js",
	"author": "KAnggara75",
	"license": "MIT",
	"version": "0.1.0",
	"description": "WhatsApp API by KAnggara75",
	"keywords": [
		"node.js",
		"pakaiwa",
		"whatsapp-api"
	],
	"homepage": "https://github.com/pakaiwa/api#readme",
	"bugs": {
		"url": "https://github.com/pakaiwa/api/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/pakaiwa/api.git"
	},
	"scripts": {
		"build": "tsc && tsc-alias",
		"start": "nodemon dist/app.js",
		"start:dev": "node dist/app.js",
		"seed": "pnpm prisma db seed",
		"wa": "pnpm --hot src/app/whatsapp.ts",
		"test": "jest -i --detectOpenHandles --runInBand",
		"mig": "pnpm prisma generate && pnpm prisma migrate dev --name init && pnpm seed",
		"fc": "pnpm prettier --write . && pnpm eslint --fix . && pnpm prettier --check . && pnpm eslint ."
	},
	"prisma": {
		"seed": "pnpm src/database/seed/seeder.ts",
		"schema": "src/database/schema.prisma"
	},
	"dependencies": {
		"@prisma/client": "^5.16.0",
		"dotenv": "^16.4.5",
		"express": "^4.19.2",
		"express-rate-limit": "^7.3.1",
		"helmet": "^7.1.0",
		"uuid": "^10.0.0",
		"winston": "^3.13.0",
		"zod": "^3.23.8"
	},
	"devDependencies": {
		"@babel/preset-env": "^7.24.7",
		"@babel/preset-typescript": "^7.24.7",
		"@eslint/js": "^9.5.0",
		"@types/express": "^4.17.21",
		"@types/supertest": "^6.0.2",
		"@types/uuid": "^10.0.0",
		"babel-jest": "^29.7.0",
		"globals": "^15.6.0",
		"jest": "^29.7.0",
		"prettier": "^3.3.2",
		"prisma": "^5.16.0",
		"supertest": "^7.0.0",
		"tsc-alias": "^1.8.10",
		"typescript": "^5.5.2",
		"typescript-eslint": "^7.14.1"
	}
}
