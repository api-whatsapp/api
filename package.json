{
	"name": "pakaiwa",
	"type": "module",
	"module": "src/main.ts",
	"main": "dist/main.js",
	"author": "KAnggara75",
	"license": "MIT",
	"version": "0.1.0",
	"description": "WhatsApp API by KAnggara75",
	"keywords": [
		"node.js",
		"pakaiwa",
		"whatsapp-api"
	],
	"homepage": "https://github.com/pakaiwa/api#readme",
	"bugs": {
		"url": "https://github.com/pakaiwa/api/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/pakaiwa/api.git"
	},
	"scripts": {
		"build": "tsc && tsc-alias",
		"start": "node dist/main.js",
		"dev": "nodemon --signal SIGHUP src/main.ts",
		"seed": "pnpm prisma db seed",
		"wa": "nodemon src/app/whatsapp.ts",
		"test": "jest -i --detectOpenHandles --runInBand",
		"mig": "rm -rf src/database/migrations && pnpm prisma generate && pnpm prisma migrate dev --name init && pnpm seed",
		"fc": "pnpm prettier --write . && pnpm eslint --fix . && pnpm prettier --check . && pnpm eslint ."
	},
	"prisma": {
		"seed": "tsx src/database/seed/seeder.ts",
		"schema": "src/database/schema.prisma"
	},
	"dependencies": {
		"@hapi/boom": "^10.0.1",
		"@prisma/client": "^5.16.1",
		"@whiskeysockets/baileys": "^6.7.5",
		"compression": "^1.7.4",
		"cors": "^2.8.5",
		"dotenv": "^16.4.5",
		"express": "^4.19.2",
		"express-rate-limit": "^7.3.1",
		"helmet": "^7.1.0",
		"link-preview-js": "^3.0.5",
		"pino": "^9.2.0",
		"qrcode-terminal": "^0.12.0",
		"uuid": "^10.0.0",
		"winston": "^3.13.0",
		"zod": "^3.23.8"
	},
	"devDependencies": {
		"@babel/preset-env": "^7.24.7",
		"@babel/preset-typescript": "^7.24.7",
		"@eslint/js": "^9.6.0",
		"@types/compression": "^1.7.5",
		"@types/cors": "^2.8.17",
		"@types/express": "^4.17.21",
		"@types/supertest": "^6.0.2",
		"@types/uuid": "^10.0.0",
		"babel-jest": "^29.7.0",
		"globals": "^15.6.0",
		"jest": "^29.7.0",
		"prettier": "^3.3.2",
		"prisma": "^5.16.1",
		"supertest": "^7.0.0",
		"tsc-alias": "^1.8.10",
		"tsx": "^4.16.0",
		"typescript": "^5.5.2",
		"typescript-eslint": "^7.14.1"
	}
}
