{
	"name": "pakaiwa",
	"version": "0.1.0-alpha0359",
	"description": "WhatsApp API by PakaiWA",
	"keywords": [
		"node.js",
		"pakaiwa",
		"whatsapp-api"
	],
	"homepage": "https://pakaiwa.my.id",
	"bugs": {
		"url": "https://github.com/pakaiwa/api/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/pakaiwa/api.git"
	},
	"license": "MIT",
	"author": "KAnggara75",
	"type": "module",
	"main": "dist/main.ts",
	"scripts": {
		"build": "rm -rf dist && tsc && tsc-alias",
		"dev": "nodemon --signal SIGHUP src/main.ts",
		"fc": "pnpm prettier --write . && pnpm eslint --fix . && pnpm prettier --check . && pnpm eslint .",
		"mig": "rm -rf src/database/migrations && pnpm prisma generate && pnpm prisma migrate dev --name init && pnpm seed",
		"seed": "pnpm prisma db seed",
		"start": "node dist/main.js",
		"test": "jest -i --detectOpenHandles --runInBand"
	},
	"dependencies": {
		"@hapi/boom": "^10.0.1",
		"@prisma/client": "^5.19.0",
		"@whiskeysockets/baileys": "^6.7.7",
		"compression": "^1.7.4",
		"cors": "^2.8.5",
		"dotenv": "^16.4.5",
		"express": "^4.19.2",
		"express-rate-limit": "^7.4.0",
		"helmet": "^7.1.0",
		"link-preview-js": "^3.0.5",
		"pino": "^9.3.2",
		"prisma": "^5.19.0",
		"qrcode": "^1.5.4",
		"qrcode-terminal": "^0.12.0",
		"typescript": "^5.5.4",
		"uuid": "^10.0.0",
		"winston": "^3.14.2",
		"zod": "^3.23.8"
	},
	"devDependencies": {
		"@babel/preset-env": "^7.25.4",
		"@babel/preset-typescript": "^7.24.7",
		"@eslint/js": "^9.9.1",
		"@jest/globals": "^29.7.0",
		"@types/compression": "^1.7.5",
		"@types/cors": "^2.8.17",
		"@types/eslint__js": "^8.42.3",
		"@types/express": "^4.17.21",
		"@types/jest": "^29.5.12",
		"@types/node": "^22.5.2",
		"@types/qrcode": "^1.5.5",
		"@types/supertest": "^6.0.2",
		"@types/uuid": "^10.0.0",
		"babel-jest": "^29.7.0",
		"globals": "^15.9.0",
		"jest": "^29.7.0",
		"pino-http": "^10.2.0",
		"prettier": "^3.3.3",
		"supertest": "^7.0.0",
		"ts-jest": "^29.2.5",
		"tsc-alias": "^1.8.10",
		"tsx": "^4.19.0",
		"typescript-eslint": "^8.4.0"
	},
	"prisma": {
		"seed": "tsx src/database/seed/seeder.ts",
		"schema": "src/database/schema.prisma"
	}
}
